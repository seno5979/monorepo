{
  "name" : "crp",
  "$schema" : "../../node_modules/nx/schemas/project-schema.json",
  "projectType" : "application",
  "sourceRoot" : "services/crp/src",
  "targets" : {
    "build" : {
      "executor" : "@nx/gradle:build",
      "options" : {
        "outputPath" : "dist/services/crp"
      }
    },
    "test" : {
      "executor" : "@nx/gradle:test"
    },
    "lint" : {
      "executor" : "@nx/linter:eslint"
    },
    "docker-build" : {
      "executor" : "nx:run-commands",
      "options" : {
        "command" : "docker build -t crp:latest -f services/crp/Dockerfile ."
      }
    },
    "deploy" : {
      "executor" : "nx:run-commands",
      "dependsOn" : [ "build", "docker-build" ],
      "options" : {
        "command" : "helm upgrade --install crp ./helm/crp"
      }
    }
  },
  "tags" : [ "type:service", "scope:backend", "team:publisher-team", "tier:backend" ]
}